    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.12",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
